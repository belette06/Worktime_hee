

  <div class="border p-3 mb-4 container">
    <%= simple_form_for(@weektime) do |f| %>
      <%= f.error_notification %>
      <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>
    
    
    
        <%= f.input :dateweek, as: :date, html5: true, label: "Selectionne le jour de la semaine " , hint: "selection le jour d'une semaine"%>
      
    
      <div class="form-actions text-center ">
        <%= f.button :submit %>
      </div>
    <% end %>
  </div>
</br>