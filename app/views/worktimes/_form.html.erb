<div class="border p-3 mb-4 container">
  <div class="row">
    <%= simple_form_for [@weektime, @worktime] do |f| %>
      <%= f.error_notification %>
      <%#= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>
      <div class="col-sm">  </div> 
      <div class="col-sm">  
        <%= f.input :gotime, as: :time, html5: true, :label => "Heure de demarrage " %>  
        <%= f.input :endtime, as: :time, html5: true , :label => "Heure de fin " %>
      </div>
      
      </br>

      <div class="form-actions">
        <%= f.button :submit, data: {turbo: false}  %>
      </div>

      <div class="col-sm">  </div> 
    <% end %>
  </div>
</div>
