<div >
 
  <h1 class="text-center">New worktime</h1>
</div>



<%= simple_form_for [@weektime, @worktime] do |f| %>
  <%= f.error_notification %>
  <%#= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :gotime, as: :time, html5: true %>
    <%= f.input :endtime, as: :time, html5: true  %>
    <%#= f.input :accord %>
    
  </div>

  <div class="form-actions">
    <%= f.button :submit, data: {turbo: false}  %>
  </div>
<% end %>


<br>

<div>
  <%#= link_to "Back to worktimes", weektime_path(@weektime) %>
</div>
